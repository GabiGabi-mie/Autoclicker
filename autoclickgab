Sexy Auto clicker

-- // GUI Setup
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

local Frame = Instance.new("Frame")
Frame.Size = UDim2.new(0, 200, 0, 100)
Frame.Position = UDim2.new(0.1, 0, 0.1, 0)
Frame.BackgroundColor3 = Color3.fromRGB(35, 35, 35)
Frame.BorderSizePixel = 0
Frame.Active = true
Frame.Draggable = true
Frame.Parent = ScreenGui

local ToggleButton = Instance.new("TextButton")
ToggleButton.Size = UDim2.new(1, -20, 0, 40)
ToggleButton.Position = UDim2.new(0, 10, 0, 30)
ToggleButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
ToggleButton.Text = "Auto Click: OFF"
ToggleButton.Parent = Frame

-- // Auto Clicker Logic (Safe - won't trigger prompts)
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")

local autoClicking = false
local clickDelay = 0.1 -- adjust speed here

-- Function to simulate only mouse clicks (not inputs that prompts detect)
local function doClick()
    local player = game.Players.LocalPlayer
    local mouse = player:GetMouse()
    
    -- Fire click directly on the target if it exists
    if mouse.Target then
        mouse1click() -- This only simulates an actual mouse click, no prompt input
    end
end

ToggleButton.MouseButton1Click:Connect(function()
    autoClicking = not autoClicking
    ToggleButton.Text = "Auto Click: " .. (autoClicking and "ON" or "OFF")

    if autoClicking then
        task.spawn(function()
            while autoClicking do
                doClick()
                task.wait(clickDelay)
            end
        end)
    end
end)
